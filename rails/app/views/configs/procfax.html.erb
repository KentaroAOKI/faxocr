<h1>procfaxログ</h1>
<p>Log Directory Path: <%= @logdir_path %></p>
<p><%= link_to "procfax.sh実行", procfax_exec_configs_path %></p>
<ul>
  <% @files.each do |file| %>
    <% if File.ftype(file) == "directory" %>
    <li><%= File.basename(file).gsub(/(\d{4})(\d{2})(\d{2})(\d{2})(\d{2})/, '\1/\2/\3-\4:\5') %></li>
    <% end %>
  <% end %>
</ul>
